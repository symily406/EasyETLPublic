<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easy.etl.machine.mapper.MachineHardDiskMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="machineHardDiskResultMap" type="com.easy.etl.machine.entity.MachineHardDisk">
        <id column="HARD_DISK_ID" property="hardDiskId"/>
        <result column="MACHINE_ID" property="machineId"/>
        <result column="DIR_NAME" property="dirName"/>
        <result column="SYS_TYPE_NAME" property="sysTypeName"/>
        <result column="TYPE_NAME" property="typeName"/>
        <result column="TOTAL" property="total"/>
        <result column="FREE" property="free"/>
        <result column="USED" property="used"/>
        <result column="USAGE_RATE" property="usageRate"/>

    </resultMap>

</mapper>
